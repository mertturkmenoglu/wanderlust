{
  "name": "api",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "clerk-sync": "bun run src/scripts/clerk-sync.ts",
    "generate": "drizzle-kit generate --config=src/db/drizzle.config.ts",
    "ngrok": "ngrok http --domain=welcomed-communal-raven.ngrok-free.app 5000",
    "env:pull": "bunx dotenv-vault@latest pull",
    "env:push": "bunx dotenv-vault@latest push",
    "studio": "bunx drizzle-kit studio --config=src/db/drizzle.config.ts --verbose",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.5",
    "@clerk/backend": "^1.1.2",
    "@hono/clerk-auth": "^2.0.0",
    "@hono/zod-validator": "^0.2.1",
    "@poppinss/cliui": "^6.4.1",
    "amqplib": "^0.10.4",
    "drizzle-orm": "^0.30.10",
    "drizzle-zod": "^0.5.1",
    "envalid": "^8.0.0",
    "hono": "4.3.7",
    "hono-rate-limiter": "^0.3.0",
    "ioredis": "^5.4.1",
    "minio": "^8.0.0",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "svix": "^1.24.0",
    "typesense": "^1.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/bun": "latest",
    "drizzle-kit": "^0.21.2"
  }
